    {
      "id": "fraud_pattern_indicators",
      "name": "Fraud Pattern Indicators",
      "type": "anomaly_detection",
      "fields": ["transactionType", "amount", "timestamp", "location"],
      "pattern": {
        "high_risk_indicators": [
          {
            "condition": "amount > 5000 && transactionType === 'debit'",
